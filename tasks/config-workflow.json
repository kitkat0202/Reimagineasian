{
  "jsLintIgnore": "./src/assets/js/vendors/**",
  "jsSrc": "./src/assets/js/**/*.js",
  "jsComponents": {
    "vendors": {
      "process": false,
      "filelist":[
        "./src/assets/js/vendors/*.js"
      ],
      "filename": "vendors.js"
    },
    "theme": {
      "process": true,
      "filelist":[
        "./src/assets/js/*.js"
      ],
      "filename":"main.js"
    }
  },
  "src": {
    "sassSrc": "./src/assets/scss/*.scss",
    "sassSrc2": "./src/assets/scss/**/*.scss",
    "imgSrc": "./src/assets/img/**",
    "jsonSrc": "./src/assets/json/**"
  },
  "handlebars": {
    "hbsSrc": "./src/**/*.html",
    "hbsComponents": "./src/**/**/*.{hbs,js,json}",
    "hbsTemplates": "./src/_hbs-templates",
    "hbsData": "./src/_hbs-templates/data/**/*.{js,json}",
    "hbsDecorators": "",
    "hbsHelpers": "./src/_hbs-templates/helpers/*.js",
    "hbsPartials": "./src/_hbs-templates/partials/**/*.hbs"
  },
  "build": {
    "root": "./build/",
    "css": "./build/assets/css",
    "cssName": "main.css",
    "cssSrc": "./build/assets/css/*.css",
    "cssMapSrc": "./build/assets/css/*.map",
    "img": "./build/assets/img",
    "imgSrc": "./build/assets/img/**",
    "js": "./build/assets/js",
    "jsSrc": "./build/assets/js/*.js",
    "jsMapSrc": "./build/assets/js/*.map",
    "htmlSrc": "./build/**/*.html",
    "json": "./build/assets/json/",
    "assetsSrc": [
      "./build/**",
      "!./build/assets/css/*.map",
      "!./build/assets/js/*.map"
    ]
  }
}
